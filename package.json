{
    "name": "create-limabean-app",
    "version": "0.0.4",
    "description": "A CLI tool to generate limabean apps",
    "type": "module",
    "module": "src/index.ts",
    "bin": {
        "create-limabean-app": "./generate-app.js"
    },
    "scripts": {
        "start": "vite",
        "test": "vitest run --environment jsdom",
        "test:coverage": "vitest run --environment jsdom --coverage",
        "build": "vite build && node scripts/post-build.js",
        "lint": "eslint .",
        "format": "prettier --write ."
    },
    "author": "Brian Bal",
    "license": "MIT",
    "dependencies": {
        "limabean": "^0.0.4"
    },
    "devDependencies": {
        "@eslint/js": "^9.6.0",
        "@types/eslint__js": "^8.42.3",
        "eslint": "^8.57.0",
        "globals": "^15.7.0",
        "jsdom": "^24.1.0",
        "prettier": "^3.3.2",
        "typescript": "^5.5.2",
        "typescript-eslint": "^7.14.1",
        "unimport": "^3.7.2",
        "vite": "^5.3.2",
        "vitest": "^1.6.0"
    }
}
